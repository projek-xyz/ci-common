{
    "name": "projek-xyz/ci-common",
    "description": "Common Module for Projek.xyz CI3 Starter Kit",
    "homepage": "http://www.projek.xyz/ci-common",
    "type": "projek-ci-module",
    "license": "MIT",
    "keywords": [
        "codeigniter", "projek-xyz-ci", "projek-ci-module", "module"
    ],
    "authors": [
        {
            "name": "Fery Wardiyanto",
            "email": "feryardiyanto@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/projek-xyz/ci-common/issues",
        "source": "https://github.com/projek-xyz/ci-common"
    },
    "autoload": {
        "psr-4": {
            "Projek\\CI\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Projek\\CI\\Common\\Tests\\": "tests/src/"
        }
    },
    "extra": {
        "projek-module-name": "common"
    },
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "projek-xyz/ci-installer": "dev-master",
        "projek-xyz/ci-console": "dev-master",
        "league/plates": "~3.1",
        "markstory/mini-asset": "dev-master",
        "vlucas/phpdotenv": "^2.0"
    },
    "require-dev": {
        "codeigniter/framework": "^3.0",
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
